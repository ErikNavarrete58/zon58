{{#each ediciones}}
<div class="container p-4">
  <div class="row">
     <div class="col-md-5 mx-auto">
         <div class="card">
            <div class="card-body">
               <form action="/admin/eds23ediciongoleadores/{{id_registro}}" method="POST">
              
         
            
              <div class="form-row">
           <div class="col">

              <div class="form-group">
                  <h5>Jornada</h5>
               <input type="text" value="{{Jornada}}" name="Jornada" class="form-control" placeholder="Jornada">
              </div>

         </div>
             <div class="col">
               <div class="form-group">
                   <h5>Fecha</h5>
               <input type="text" value="{{Fecha}}" name="Fecha" class="form-control" placeholder="Fecha">
              </div>


             </div>
    </div>
            
              
               <div class="col">
               <div class="form-group">
                   <h5>Hora</h5>
               <input type="text" value="{{Hora}}" name="hora" class="form-control" placeholder="hora">
               </div>
               </div>



<div class="form-row">
         <div class="col">

           <div class="form-group">
               <h5>ID</h5>
               <input type="text" value="{{id}}" name="id" class="form-control" placeholder="GF">
              </div>
              </div>
           <div class="col">
               <div class="form-group">
                   <h5>Nombre</h5>
               <input type="text" value="{{nombre}}" name="nombre" class="form-control" placeholder="GC">
              </div>
              </div>
            <div class="col">
